---
import GalleryLayout from "../components/react/Layout/GalleryLayout.jsx";
import RootLayout from "./RootLayout.astro";
const { frontmatter } = Astro.props;

const pageTitle = frontmatter
    ? frontmatter?.title
        ? frontmatter.title
        : Astro.props.title
    : Astro.props.title;
---

<RootLayout title={pageTitle}>
    <GalleryLayout title={pageTitle} client:load>
        <slot />
    </GalleryLayout>
</RootLayout>
